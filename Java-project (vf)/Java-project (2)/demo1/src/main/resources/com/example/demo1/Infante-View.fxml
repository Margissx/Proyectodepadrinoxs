<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.demo1.InfanteController"
            prefHeight="564.0" prefWidth="429.0">
    <children>
        <Pane prefHeight="575.0" prefWidth="429.0" style="-fx-background-color: #0C39FF;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ImageView fitHeight="52.0" fitWidth="52.0" layoutX="75.0" layoutY="51.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../demo1/img/user.png" />
                    </image>
                </ImageView>
                <Label alignment="CENTER" layoutX="140.0" layoutY="54.0" prefHeight="23.0" prefWidth="173.0" text="Menu Infante" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="18.0" />
                    </font>
                </Label>
                <Label alignment="CENTER" layoutX="148.0" layoutY="77.0" prefHeight="23.0" prefWidth="114.0" text="Enviar Carta" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="12.0" />
                    </font>
                </Label>
                <TextField fx:id="txtDirPath" layoutX="76.0" layoutY="200.0" prefHeight="34.0" prefWidth="218.0" promptText="Direccion de la carta" />
                <Button fx:id="btnFile" layoutX="304.0" layoutY="200.0" mnemonicParsing="false" onMouseClicked="#ShowDialog" prefHeight="32.0" prefWidth="50" styleClass="button2" stylesheets="@../demo1/styles/fullpack.css" text="Buscar" />

                <Button fx:id="btnEnviar" layoutX="76.0" layoutY="395.0" mnemonicParsing="false" onMouseClicked="#CopyFile" prefHeight="32.0" prefWidth="278.0" styleClass="buttonagree" stylesheets="@../demo1/styles/fullpack.css" text="Enviar Carta" />
                <Button fx:id="btnSalir" layoutX="76.0" layoutY="454.0" mnemonicParsing="false"  prefHeight="32.0" prefWidth="278.0" onMouseClicked="#exit" styleClass="buttonSalir" stylesheets="@../demo1/styles/fullpack.css" text="Salir" />
            </children>
        </Pane>



    </children>

</AnchorPane>
